{
  "name": "alexander-roode",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run debug",
    "build-ts": "tsc",
    "build-webpack": "webpack --mode production",
    "build": "npm run build-webpack && npm run build-ts && npm run tslint",
    "watch-ts": "ts-node-dev --respawn --no-notify --inspect --transpile-only --rs --watch src/music/pieces/*.md,src/music/music.json src/server.ts",
    "watch-webpack": "webpack -w",
    "debug": "concurrently -k -p \"[{name}]\" -n \"Webpack,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-webpack\" \"npm run watch-ts\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "bootstrap": "^4.4.1",
    "config": "^3.3.6",
    "express": "~4.17.1",
    "express-promise-router": "^4.1.0",
    "express-recaptcha": "^5.0.2",
    "form-data": "^2.3.3",
    "fs-extra": "^10.0.0",
    "jquery": "^3.6.0",
    "mailgun.js": "^3.5.7",
    "marked": "^3.0.0",
    "moment": "^2.29.1",
    "morgan": "~1.10.0",
    "parsleyjs": "^2.9.2",
    "popper.js": "^1.16.1",
    "pug": "^3.0.2",
    "serve-favicon": "~2.5.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/config": "0.0.39",
    "@types/express": "^4.17.13",
    "@types/form-data": "^2.5.0",
    "@types/fs-extra": "^9.0.12",
    "@types/jquery": "^3.5.6",
    "@types/node": "^16.6.2",
    "concurrently": "^6.2.1",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.2.0",
    "node-sass": "^6.0.1",
    "sass-loader": "^12.1.0",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  },
  "author": "Alexander Roode",
