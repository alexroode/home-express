{
  "name": "alexander-roode",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run debug",
    "build-ts": "tsc",
    "build-webpack": "webpack -p",
    "build": "npm run build-webpack && npm run build-ts && npm run tslint",
    "watch-ts": "ts-node-dev --respawn --no-notify --inspect --transpile-only --rs --watch src/music/pieces/*.md,src/music/music.json src/server.ts",
    "watch-webpack": "webpack -w",
    "debug": "concurrently -k -p \"[{name}]\" -n \"Webpack,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-webpack\" \"npm run watch-ts\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@types/mailgun-js": "^0.22.11",
    "bootstrap": "^4.4.1",
    "config": "^3.3.1",
    "express": "~4.17.1",
    "express-promise-router": "^4.1.0",
    "express-recaptcha": "^5.0.1",
    "fs-extra": "^10.0.0",
    "jquery": "^3.5.1",
    "mailgun-js": "^0.22.0",
    "marked": "^2.0.0",
    "moment": "^2.24.0",
    "morgan": "~1.10.0",
    "parsleyjs": "^2.9.2",
    "popper.js": "^1.16.1",
    "pug": "^3.0.1",
    "serve-favicon": "~2.5.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/config": "0.0.38",
    "@types/express": "^4.17.6",
    "@types/fs-extra": "^9.0.11",
    "@types/jquery": "^3.3.35",
    "@types/node": "^15.6.1",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.0",
    "sass-loader": "^11.1.1",
    "ts-node-dev": "^1.1.6",
    "tslint": "^6.1.3",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
